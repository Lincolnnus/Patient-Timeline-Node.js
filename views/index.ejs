 	<% if (!user) { %>
 	<div class="hero-unit">  
	<h1>Welcome to Timeline Viewer</h1> 
	<p>Timeline Viewer is Based on Node.js</p>   
	<p>  
		<a class="btn btn-primary btn-large" href="/login">Login</a>
	    <a class="btn btn-large" href="/reg">Register</a> 
	</p> 
	</div> 
    <% } else if(user.xml!=null) { %>
    <div class="row-fluid">
		 <div class="span3">
			<nav>
			  <ol class="sidenav fixed" id="nav">
			    <li>
			      <a href="#demographics">Demographics</a>
			    </li>
			    <li>
			      <a href="#allergyList">Allergies</a>
			    </li>
			    <li>
			      <a href="#immunizationList">Immunizations</a>
			    </li>
			    <li>
			      <a href="#medicationList">Medications</a>
			    </li>
			    <li>
			      <a href="#labList">Labs</a>
			    </li>
			    <li>
			      <a href="#encounterList">Encounters</a>
			    </li>
			    <li>
			      <a href="#problemList">Problems</a>
			    </li>
			    <li>
			      <a href="#procedureList">Procedures</a>
			    </li>
			    <li>
			      <a href="#vitalList">Vitals</a>
			    </li>
			   </ol>
			</nav>
		</div>
		<div class="span9">
			<div id="demographics">
			  <div class="hero-unit">  
					<h1 class="family"><%= demographics.name.family %> 
				    <%- partial('listGivenName', demographics.name.given) %></h1> 
					<p class="dob">Date of Birth:<%= demographics.dob.substr(0,10) %></p> 
				    <p class="gender">
				    	<% if (demographics.gender == "Male") {%>
				    		<img src="/images/male.jpg" width="100px">
				    	<%} else {%>
				    		<img src="/images/female.jpg" width="100px">
				    	<%}%>
				    </p>
				    <p class="marital">Marital Status:<%= demographics.marital_status %></p>
				</div> 
				<div class="row">
				    <div class="address span4">
					    <h2> <%- partial('listStreet', demographics.address.street) %></h2>
					    <p> <%= demographics.address.city %>,<%= demographics.address.state %>,<%= demographics.address.country %>.</p>
					    <p><%= demographics.address.zip %></p>
					    <p class="phoneHome"><%= demographics.phone.home %></p>
				   
				    </div>
				    <div class="span4">
				    	<h2>Other Information</h2>
				    	<p class="race"><%= demographics.race %></p>
				    	<p class="ethnicity"><%= demographics.ethnicity %></p>
					    <p class="religion"><%= demographics.religion %></p>
				    </div>
				</div>
			</div>
			<div id="allergyList">
			  <div class="hero-unit">
			    <h1>Allergies</h1>
			    <div class="row">
			   		<%- partial('listAllergy', allergies) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="immunizationList">
			  <div class="hero-unit">
			    <h1>Immunizations</h1>
			    <div class="row">
			    <%- partial('listImmunization', immunizations) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="medicationList">
			  <div class="hero-unit">
			    <h1>Medications</h1>
			    <div class="row">
			   		<%- partial('listMedication', medications) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="labList">
			 <div class="hero-unit">
			    <h1>Labs</h1>
			    <div class="row">
			    <%- partial('listLab', labs) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="encounterList">
			 <div class="hero-unit">
			 <h1>Encounters</h1>
			    <div class="row">
			    <%- partial('listEncounter', encounters) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="problemList">
			 <div class="hero-unit">
				 <h1>Problems</h1>
			    <div class="row">
			    <%- partial('listProblem', problems) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="procedureList">
			 <div class="hero-unit">
				 <h1>Procedures</h1>
			    <div class="row">
			    <%- partial('listProcedure', procedures) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
			<div id="vitalList">
			 <div class="hero-unit">
				 <h1>Vitals</h1>
			    <div class="row">
			    <%- partial('listVital', vitals) %>
			    </div>
			  </div>
			  <div class="row-fluid"></div>
			</div>
		</div>
	</div>
	<% } else { %> 
		<a class="btn btn-primary btn-large" href="/upload">Upload New CCDA Document</a>
	<% } %>